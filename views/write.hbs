<p>Hello {{user.firstname}} {{user.lastname}} {{user.email}} {{user.id}}</p>

<form id="storyForm" data-userId="{{user.id}}">
  <div class="row">
    <div class="input-field col s6">
      <input id="storyTitle" type="text" class="validate">
      <label class="active" for="story-title">Story Title</label>
    </div>
  </div>
  <div class="row">
    <form class="col s12">
      <div class="row">
        <div class="input-field col s12">
          <textarea id="body" class="materialize-textarea"></textarea>
          <label for="story">The Story</label>
          <span class="helper-text" data-error="wrong" data-success="right">Your time to shine.</span>
        </div>
      </div>
    </form>
  </div>
  <input id="submit" type="submit" value="Submit" class="btn waves-effect waves-teal">
</form>


<!-- Custom Script -->
<script src="../public/js/write.js" type="text/javascript"></script>
<script type="application/javascript" src="/js/app.js" type="text/javascript"></script>